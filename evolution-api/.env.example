# Evolution API - Configurações

# IMPORTANTE: Gere uma API Key forte e única
# Você pode usar: openssl rand -base64 32
EVOLUTION_API_KEY=MUDE_ISSO_POR_UMA_CHAVE_FORTE_E_UNICA

# URL da Evolution API (local)
EVOLUTION_API_URL=http://localhost:8080

# Configurações do PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=evolution

# pgAdmin (opcional)
PGADMIN_EMAIL=admin@agendahof.com
PGADMIN_PASSWORD=admin

# Nome da instância WhatsApp
INSTANCE_NAME=agendahof
